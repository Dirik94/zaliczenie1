<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ThirdScreen">

  <EditText
      android:layout_marginTop="16dp"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:id="@+id/textBox1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_below="@id/textBox1"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:id="@+id/textBox2"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_below="@id/textBox2"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:id="@+id/textBox3"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:layout_below="@id/textBox3"
      android:id="@+id/textBox4"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">
  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_below="@id/textBox4"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:id="@+id/textBox5"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:layout_below="@id/textBox5"
      android:id="@+id/textBox6"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">
  </EditText>

  <EditText
      android:layout_marginTop="16dp"
      android:layout_below="@id/textBox6"
      android:layout_marginLeft="16dp"
      android:layout_marginRight="16dp"
      android:id="@+id/textBox7"
      android:layout_width="match_parent"
      android:layout_height="wrap_content">

  </EditText>

  </RelativeLayout>